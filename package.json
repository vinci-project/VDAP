{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "stem": "nodemon src/stem/vdap.js",
    "backend": "concurrently \"npm run server\" \"npm run stem\" \"npm run storagebe\" \"npm run blockchain-sim\"",
    "storagebe": "nodemon src/storage/index.js --port=9000,9001 --id=fb99337c-1bbb-4e45-8e52-2f951776f313,040c5160-8a22-4a0b-9aa8-efa15614ed03",
    "blockchain-sim": "nodemon src/blockchain-sim/index.js",
    "deploy": "gh-pages -d dist",
    "test": "mocha",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run stem\" \"npm run storagebe\" \"npm run blockchain-sim\""
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "exceljs": "^1.6.3",
    "express": "^4.16.3",
    "ldapjs": "^1.0.2",
    "lowdb": "^1.0.0",
    "minimist": "^1.2.0",
    "promised-ldap": "^0.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-perfect-scrollbar": "^1.4.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "shortid": "^2.2.14",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
